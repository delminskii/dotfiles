alias downloads='cd $HOME/downloads/ && ll ./'
alias tmp='cd /tmp'
alias aliases='vim $HOME/.bash_aliases'
alias make_go_dirs='mkdir src pkg bin cmd'
alias python3='/usr/bin/python3'
